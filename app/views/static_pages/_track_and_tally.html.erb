<div id='tallies-badges-container' class='container-fluid'>
  <div class='row'>
    <div id='tallies-badges'>
      <div id='tallies' class='col-xs-12 col-md-6'>
        <% categories = [
          'https://d2m6u6shu0evoe.cloudfront.net/images/deer_black.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/turkey_black.png'
          ] %>
          <% categories.each do |category| %>
            <ul>
              <header>
                <%= image_tag(category) %>
              </header>
              <% 4.times do %>
                <li>
                  <%= image_tag('https://d2m6u6shu0evoe.cloudfront.net/images/tik_black.png') %>
                </li>
              <% end %>
            </ul>
          <% end %>
      </div>
      <div id='badges' class='col-xs-12 col-md-6'>
        <% badges = [
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_bear.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_deer.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_turkey.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_canine.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_moose.png',
          'https://d2m6u6shu0evoe.cloudfront.net/images/badges/first_water_fowl.png'
          ] %>
          <ul>
          <% badges.each do |badge| %>
            <li class='badge'>
              <%= image_tag(badge) %>
            </li>
          <% end %>
          </ul>
      </div>
    </div>
  </div>
</div>
